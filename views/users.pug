extends layout

block content
  .container
    h1 #{title}
    table.table.table-bordered(style='width:100%', border='1')
      thead
        tr
          th(scope='col') Id
          th(scope='col') PhoneNumber
      tbody
        each user in users
          tr
            td(scope='row') #{user.id}
            td #{user.firstName}
            td #{user.lastName}
            td #{user.phoneNumber}
